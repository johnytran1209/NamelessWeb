@model  IEnumerable<NamelessWeb.Models.WebSystem.GuitarRating>
@{
    ViewBag.Title = "Rating";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Rating</h2>

<body>
    <div>
        <ul>
            @foreach (var item in Model)
            {
                @Html.Display(item.CusName);
                @Html.Display(item.Stars.ToString())
                @Html.Display(item.FeedMes);
            }
        </ul>   
    </div>
</body>

