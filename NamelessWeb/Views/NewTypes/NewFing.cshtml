@model NamelessWeb.Models.Guitar.Wood.GoFing
@{
    ViewBag.Title = "View";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>New Fing</h2>
<body>
@using (Html.BeginForm("NewFing", "NewTypes"))
{
    <div class="form-group">
        <div style="left: auto" , class="form-group">
            @Html.Label("Fing Id:")
            @Html.TextBoxFor(m => m.FingId, new { @class = "form-control" })
        </div>
        <div style="left: auto" , class="form-group">
            @Html.Label("Fing:")
            @Html.TextBoxFor(m => m.FingName, new {@class = "form-control"})
        </div>
        <button type="submit" value="Save" class="btn btn-primary">Save</button>
    </div>
}
</body>
@section scripts
{
    @Scripts.Render("~/bundles/jqueryval") ;
}

