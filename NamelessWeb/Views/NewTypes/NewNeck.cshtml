@model NamelessWeb.Models.Guitar.Wood.GoNeck
@{
    ViewBag.Title = "View";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2 style = "color:aliceblue">New Neck</h2>
<body>
@using (Html.BeginForm("NewNeck", "NewTypes", FormMethod.Post, new { @class = "form-horizontal", role = "form", @style = "color:aliceblue" }))
{
    <div class="form-group">
        <div style="left: auto" , class="form-group">
            @Html.Label("Neck Id:")
            @Html.TextBoxFor(m => m.NeckId, new { @class = "form-control" })
        </div>
        <div style="left: auto" , class="form-group">
            @Html.Label("Neck:")
            @Html.TextBoxFor(m => m.NeckName, new {@class = "form-control"})
        </div>
        <button type="submit" value="Save" class="btn btn-primary">Save</button>
    </div>
}
</body>
@section scripts
{
    @Scripts.Render("~/bundles/jqueryval") ;
}

